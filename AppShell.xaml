    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ProyectoFinal_MAUI__FV__ME"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="HELP STUDENTS"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="Quien somos?" />
    <ShellContent
        Title="Objetivo"
        ContentTemplate="{DataTemplate local:Objetivos_M_F}"
        Route="Objetivos" />
    <ShellContent
        Title="Recomendacion"
        ContentTemplate="{DataTemplate local:RegistroPage}"
        Route="Profes" />

</Shell>
